
.user {
    width: 60px;
    overflow: hidden;
    text-align: center;
    padding: 5px;
    color: #666;
    font-size: 12px;
    display: inline-block;
    margin: 5px;
}

.active {
    border: solid 1px #2d8cf0;
    border-radius: 5px;
}

.user image {
    width: 40px;
    height: 40px;
    border-radius: 20px;
}